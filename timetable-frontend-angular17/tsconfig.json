{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./out-tsc",
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "types": [],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "useDefineForClassFields": false,
    "lib": [
      "es2020",
      "dom"
    ]
  }
}